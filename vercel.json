{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "server.js" }
  ],
  "functions": {
    "server.js": {
      "includeFiles": "dist/**",
      "memory": 1024,
      "maxDuration": 60,
      "runtime": "nodejs20.x"
    }
  }
}
